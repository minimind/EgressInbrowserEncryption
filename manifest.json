{
  "name": "Example Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","jquery-3.1.0.min.js","content.js"],
      "run_at": "document_end"
    }
  ],
   "permissions": [
    "webRequest",
    "webRequestBlocking",
    "identity",
    "*://*.google.com/*"
  ],
  "background": {
    "scripts": ["base64.js", "background.js"]
  },
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/Logomakr_19MEHn.png",
    "default_popup": "popup.html"
  }
}
