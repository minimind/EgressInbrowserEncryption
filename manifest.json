{
  "name": "Example Gmail Extension",
  "description": "Example extension showing use of the Streak Gmail SDK",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["inboxsdk.js","content.js"],
      "run_at": "document_end"
    }
   ],
  "background": {
    "scripts": ["base64.js", "background.js"]
  },
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "images/Logomakr_19MEHn.png"
  },
  "oauth2": {
    "client_id": "939742987720-e377t00erh1q68abm9iidamcvku3sis9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/gmail.modify"
    ]
  }
}
